{
  "title": "Kokoro FastAPI Serverless",
  "description": "High-quality Text-to-Speech with voice mixing, timestamps, and phonemes. 100% API compatible with Kokoro FastAPI. Optimized for fast inference.",
  "type": "serverless",
  "category": "audio",
  "iconUrl": "https://github.com/remsky/Kokoro-FastAPI/blob/master/githubbanner.png?raw=true",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 10,
    "env": [
      {
        "key": "HF_HOME",
        "value": "/runpod-volume"
      },
      {
        "key": "TRANSFORMERS_CACHE",
        "value": "/runpod-volume"
      },
      {
        "key": "HF_HUB_CACHE",
        "value": "/runpod-volume"
      }
    ]
  }
}