{
  "title": "Kokoro FastAPI Serverless",
  "description": "High-quality Text-to-Speech with voice mixing, timestamps, and phonemes. 100% API compatible with Kokoro FastAPI. Optimized for fast inference.",
  "type": "serverless",
  "category": "audio",
  "iconUrl": "https://cdn.prod.website-files.com/67d20fb9f56ff2ec6a7a657d/685b40411e38197aff6351df_poddy-3.webp",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 10,
    "env": [
      {
        "key": "HF_HOME",
        "value": "/runpod-volume"
      },
      {
        "key": "TRANSFORMERS_CACHE",
        "value": "/runpod-volume"
      },
      {
        "key": "HF_HUB_CACHE",
        "value": "/runpod-volume"
      }
    ]
  }
}
